<%= form_for(:session, url: sessions_path) do |f| %>
	<fieldset>
		<legend>Log In</legend>
			<%= f.label :username %>
			<%= f.text_field :username, placeholder: "username" %>
			<%= f.label :password %>
			<%= f.password_field :password %> <br> <br>
			<%= f.submit "Log In", id: "submit-button", class: "btn btn-large btn-primary" %>
	</fieldset>
<% end %>